{
    "name": "intergo/sms.to-laravel-lumen",
    "description": "Laravel 5 package for sending sms in SMS.to",
    "keywords": [
        "laravel",
        "sms",
        "bulksms",
        "marketing"
    ],
    "authors": [
        {
            "name": "Intergo Telecom",
            "email": "italosm@intergo.com.cy",
            "homepage": "https://intergo.com.cy",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "7.*",
        "intergo/sms.to-php": "^1.0",
        "illuminate/support": "^8.75 || ^9.24 || ^10.0"
    },
    "autoload": {
        "psr-0": {
            "Intergo\\SmsTo\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intergo\\SmsTo\\SmsToServiceProvider"
            ]
        }
    }
}
